<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowButtonForeground" Color="AliceBlue"/>
    <SolidColorBrush x:Key="WindowButtonBackground" Color="Transparent"/>
    <SolidColorBrush x:Key="WindowButtonHighlight" Color="#647d96"/>
    <SolidColorBrush x:Key="WindowButtonPressed" Color="#3c5064"/>
    <SolidColorBrush x:Key="CloseButtonHighlight" Color="Red" />
    <SolidColorBrush x:Key="CloseButtonPressed" Color="DarkRed"/>
    <SolidColorBrush x:Key="ContentBackground" Color="#fafaff"/>

    <SolidColorBrush x:Key="CaptionForeground" Color="AliceBlue"/>

